<?xml version="1.0" encoding="UTF-8"?>

<faces-config
    xmlns="http://java.sun.com/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_1_2.xsd"
    version="1.2">
	<managed-bean>
		<managed-bean-name>quadroMB</managed-bean-name>
		<managed-bean-class>br.com.helenafranca.view.QuadroMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>categoriaMB</managed-bean-name>
		<managed-bean-class>br.com.helenafranca.view.CategoriaMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>usuarioMB</managed-bean-name>
		<managed-bean-class>br.com.helenafranca.view.UsuarioMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<managed-bean>
		<managed-bean-name>artistaMB</managed-bean-name>
		<managed-bean-class>br.com.helenafranca.view.ArtistaMB</managed-bean-class>
		<managed-bean-scope>request</managed-bean-scope>
	</managed-bean>
	<navigation-rule>
		<display-name>pages/usuario/paginaInicial</display-name>
		<from-view-id>/pages/usuario/paginaInicial.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toPesquisaQuadro</from-outcome>
			<to-view-id>/pages/usuario/pesquisaQuadro.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/usuario/paginaInicial</display-name>
		<from-view-id>/pages/usuario/paginaInicial.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toLogin</from-outcome>
			<to-view-id>/pages/usuario/login.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/homeArtista</display-name>
		<from-view-id>/pages/artista/homeArtista.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toAtualizaCadastroArtista</from-outcome>
			<to-view-id>/pages/artista/atualizaCadastroArtista.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/homeArtista</display-name>
		<from-view-id>/pages/artista/homeArtista.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toAlterarSenha</from-outcome>
			<to-view-id>/pages/artista/alterarSenha.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/homeArtista</display-name>
		<from-view-id>/pages/artista/homeArtista.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toCadastraQuadro</from-outcome>
			<to-view-id>/pages/artista/cadastraQuadro.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/homeArtista</display-name>
		<from-view-id>/pages/artista/homeArtista.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toConsultaQuadros</from-outcome>
			<to-view-id>/pages/artista/consultaQuadros.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/atualizaCadastroArtista</display-name>
		<from-view-id>/pages/artista/atualizaCadastroArtista.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toHomeArtista</from-outcome>
			<to-view-id>/pages/artista/homeArtista.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/alterarSenha</display-name>
		<from-view-id>/pages/artista/alterarSenha.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toHomeArtista</from-outcome>
			<to-view-id>/pages/artista/homeArtista.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/cadastraQuadro</display-name>
		<from-view-id>/pages/artista/cadastraQuadro.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toHomeArtista</from-outcome>
			<to-view-id>/pages/artista/homeArtista.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/consultaQuadros</display-name>
		<from-view-id>/pages/artista/consultaQuadros.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toCadastraQuadro</from-outcome>
			<to-view-id>/pages/artista/cadastraQuadro.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/consultaQuadros</display-name>
		<from-view-id>/pages/artista/consultaQuadros.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toHomeArtista</from-outcome>
			<to-view-id>/pages/artista/homeArtista.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	<navigation-rule>
		<display-name>pages/artista/atualizaQuadro</display-name>
		<from-view-id>/pages/artista/atualizaQuadro.jsp</from-view-id>
		<navigation-case>
			<from-outcome>toConsultaQuadros</from-outcome>
			<to-view-id>/pages/artista/consultaQuadros.jsp</to-view-id>
		</navigation-case>
	</navigation-rule>
	
	<validator>
  		<validator-id>alteraEmailArtistaValidator</validator-id>
  		<validator-class>br.com.helenafranca.validador.AlteraEmailArtistaValidator</validator-class>
 	</validator>

</faces-config>
